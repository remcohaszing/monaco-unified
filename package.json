{
  "name": "monaco-unified",
  "version": "1.2.0",
  "homepage": "https://monaco-unified.js.org",
  "description": "Get warnings and error messages in monaco editor based on a unified processor.",
  "type": "module",
  "workspaces": [
    "examples/*"
  ],
  "exports": {
    ".": "./index.js",
    "./worker": "./worker.js"
  },
  "files": [
    "/*.js",
    "/*.d.ts"
  ],
  "keywords": [
    "monaco",
    "monaco-editor",
    "unified",
    "unifiedjs",
    "remark",
    "rehype",
    "mdx",
    "markdown",
    "html"
  ],
  "author": "Remco Haszing <remcohaszing@gmail.com>",
  "license": "MIT",
  "repository": "remcohaszing/monaco-unified",
  "bugs": "https://github.com/remcohaszing/monaco-unified/issues",
  "funding": "https://github.com/sponsors/remcohaszing",
  "scripts": {
    "prepack": "tsc --noEmit false",
    "start": "npm --workspace demo start"
  },
  "dependencies": {
    "monaco-marker-data-provider": "^1.0.0",
    "monaco-worker-manager": "^2.0.0",
    "unified": "^10.0.0",
    "vfile": "^5.0.0"
  },
  "peerDependencies": {
    "monaco-editor": ">=0.30.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-remcohaszing": "^10.0.0",
    "monaco-editor": "^0.41.0",
    "prettier": "^3.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-remcohaszing": "^2.0.0",
    "typescript": "^5.0.0",
    "vfile-message": "^3.0.0"
  }
}
